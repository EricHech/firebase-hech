{
  "name": "firebase-soil",
  "version": "1.1.9",
  "description": "A a strongly-typed opinionated wrapper around Firebase's Real-Time Database meant to supercharge Firebase and allow for better relational management and out-of-the-box security rules.",
  "exports": {
    ".": {
      "types": "./analytics/dist/analytics/index.d.ts"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "node": {
        "require": "./dist/server/index.cjs.js",
        "import": "./dist/server/index.mjs"
      }
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "browser": {
        "require": "./dist/client/index.cjs.js",
        "import": "./dist/client/index.mjs"
      }
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EricHech/firebase-soil"
  },
  "author": "Eric Hechavarria",
  "license": "ISC",
  "devDependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "firebase": "^10.6.0",
    "firebase-admin": "^11.11.0",
    "tsup": "^7.2.0"
  }
}
