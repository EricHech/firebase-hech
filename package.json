{
  "name": "firebase-soil",
  "type": "module",
  "version": "1.1.35",
  "description": "A a strongly-typed opinionated wrapper around Firebase's Real-Time Database meant to supercharge Firebase and allow for better relational management and out-of-the-box security rules.",
  "types": "dist/types/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "require": "./dist/client.cjs",
      "types": "./dist/types/client.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs",
      "types": "./dist/types/server.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/EricHech/firebase-soil"
  },
  "author": "Eric Hechavarria",
  "license": "ISC",
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite-plugin-dts": "^3.6.3"
  },
  "dependencies": {
    "@types/node": "^20.9.0",
    "firebase": "^10.6.0",
    "firebase-admin": "^11.11.0",
    "ts-node": "^10.9.1",
    "vite": "^4.5.0"
  }
}
